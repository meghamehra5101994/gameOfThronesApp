<script type='text/javascript'>
   
var url = '<%= ajaxServerBaseURL+search%>?location=' + location+ '&type=' +type+ '&attacker_king='+attacker_king ;

    $.ajax({
            type: "get",
            url: url,
            headers: header,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function(result) {
               
                $('#zoneEditId2').empty();
                for (var i = 0; i < result.zones.length; i++) {
                    //alert(JSON.stringify(countryDetails[i]));
                    if (zoneId == result.zones[i].id) {
                        $('#zoneEditId2').append('<option value="' + result.zones[i].id + '">' + result.zones[i].name + '</option>');
                        result.zones.splice(i, 1);
                    }

                }
                if (result.zones.length !== 0) {
                    for (var i = 0; i < result.zones.length; i++) {
                        $('#zoneEditId2').append('<option value="' + result.zones[i].id + '">' + result.zones[i].name + '</option>');
                    }
                }
            },
            error: function(response) {
                if (response.readyState == 4) {
                    //console.log("Error : " + response.statusText);
                    $('#errorTitle').text('Error');
                    //planMsg
                    $('#planMsg').html(response.statusText);
                    $('#errorDialog').modal('show');
                }
                ////console.log("Error in Fetching Subscriber LCO Wise : "+JSON.stringify(response));
            }

        });
</script>